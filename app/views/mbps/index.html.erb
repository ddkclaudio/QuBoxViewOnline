<p id="notice"><%= notice %></p>

<h1>Listing Mbps</h1>

<table>
  <thead>
    <tr>
      <th>Bid price 0</th>
      <th>Bid quantity 0</th>
      <th>Ask price 0</th>
      <th>Ask quantity 0</th>
      <th>Bid price 1</th>
      <th>Bid quantity 1</th>
      <th>Ask price 1</th>
      <th>Ask quantity 1</th>
      <th>Bid price 2</th>
      <th>Bid quantity 2</th>
      <th>Ask price 2</th>
      <th>Ask quantity 2</th>
      <th>Bid price 3</th>
      <th>Bid quantity 3</th>
      <th>Ask price 3</th>
      <th>Ask quantity 3</th>
      <th>Bid price 4</th>
      <th>Bid quantity 4</th>
      <th>Ask price 4</th>
      <th>Ask quantity 4</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @mbps.each do |mbp| %>
      <tr>
        <td><%= mbp.bid_price_0 %></td>
        <td><%= mbp.bid_quantity_0 %></td>
        <td><%= mbp.ask_price_0 %></td>
        <td><%= mbp.ask_quantity_0 %></td>
        <td><%= mbp.bid_price_1 %></td>
        <td><%= mbp.bid_quantity_1 %></td>
        <td><%= mbp.ask_price_1 %></td>
        <td><%= mbp.ask_quantity_1 %></td>
        <td><%= mbp.bid_price_2 %></td>
        <td><%= mbp.bid_quantity_2 %></td>
        <td><%= mbp.ask_price_2 %></td>
        <td><%= mbp.ask_quantity_2 %></td>
        <td><%= mbp.bid_price_3 %></td>
        <td><%= mbp.bid_quantity_3 %></td>
        <td><%= mbp.ask_price_3 %></td>
        <td><%= mbp.ask_quantity_3 %></td>
        <td><%= mbp.bid_price_4 %></td>
        <td><%= mbp.bid_quantity_4 %></td>
        <td><%= mbp.ask_price_4 %></td>
        <td><%= mbp.ask_quantity_4 %></td>
        <td><%= link_to 'Show', mbp %></td>
        <td><%= link_to 'Edit', edit_mbp_path(mbp) %></td>
        <td><%= link_to 'Destroy', mbp, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Mbp', new_mbp_path %>
